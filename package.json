{
  "name": "product-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "dateformat": "^3.0.3",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "lodash.omit": "^4.5.0",
    "lodash.sortby": "^4.7.0",
    "node-sass-chokidar": "^1.3.3",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-highlight-words": "^0.14.0",
    "react-icons": "^3.0.5",
    "react-paginate": "^5.2.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-batched-actions": "^0.4.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/stylesheets/base.scss ./public/index.css --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/stylesheets/base.scss  ./public/index.css --watch --recursive --output-style compressed",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
